{
  "$schema": "http://json-schema.org/schema",
  "$id": "sqsPublisherFunction",
  "title": "SQS Publisher Function schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the function",
      "x-prompt": "What is the name of the function (no spaces)?"
    },
    "queue-name": {
      "type": "string",
      "description": "Name of the queue",
      "x-prompt": "What is the name of the queue (no spaces)?"
    }
  },
  "required": ["name", "queue-name"],
  "additionalProperties": false
}
